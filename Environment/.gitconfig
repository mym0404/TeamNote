[advice]
	addIgnoredFile = false
[alias]
	c = checkout
  w = switch
	b = branch
	cm = commit -m 
	s = status
  pl = !git pull origin $(git rev-parse --abbrev-ref HEAD)
  ps = !git push origin $(git rev-parse --abbrev-ref HEAD)
	pop = stash pop
	ada = add -A
  ad = add
  sh = show
  shh = show --name-status
  df = diff
  dff = diff HEAD
  lg1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(magenta)- %an%C(reset)%C(auto)%d%C(reset)' --all  --date-order
  lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%ai%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(magenta)- %an%C(reset)' --all  --date-order
  lg = lg1
  reflg1 = reflog --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%ai%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(bold blue)%gs%C(reset) %C(white)%s%C(reset) %C(magenta)- %an%C(reset)' --all --date-order
  reflg = reflg1
  f = fetch --all
  rs = reset
  rst = restore
  amd = commit --amend
  cp = cherry-pick
  # recz = "!sh -c 'git commit -m \"$(cat .git/COMMIT_EDITMSG)\" ' "
  ccc = "!bash -c 'git reset && git restore -SW . && git clean -df ' "
[color]
ui = auto

[commit]
verbose = true
[rerere]
enabled = true
autoupdate = true

[rebase]
autoSquash = true
autoStash = true
updateRefs = true

